{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/blog/now-env-variables-size-limit",
      "destination": "/blog/vercel-env-variables-size-limit"
    },
    {
      "source": "/blog/going-serverless-next-firebase-now",
      "destination": "/blog/nextjs-firebase-serverless"
    },
    {
      "source": "/blog/vercel-env-variables-size-limit",
      "destination": "https://vercel.com/support/articles/how-do-i-workaround-vercel-s-4-kb-environment-variables-limit"
    }
  ]
}
